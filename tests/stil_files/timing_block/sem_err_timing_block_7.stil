Signals {
    si1 In;
    si2 In;
    so1 Out;
    so2 Out;
    sio1 InOut;
    sio2 InOut;
}

SignalGroups { 
    all = 'si1 + si2 + so1 + so2 + sio1 + sio2';
}


Timing { 
  WaveformTable wft 
  { Period '100ns';
      Waveforms 
      {
        si1 {L {'10ns' L;}}
      }
  }

}


